{
  "name": "mini-memory",
  "version": "2.2.9",
  "description": "Classic memory game as custom element.",
  "main": "src/index.js",
  "author": "Emre Sakarya",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "webpack-dev-server --mode development",
    "build": "rimraf ./public && webpack -c \"webpack.config.js\" --mode production ",
    "lint": "eslint ./src",
    "serve": "browser-sync start --server \"./public\"",
    "serve-docs": "yarn doc && browser-sync start --server \"./docs\"",
    "serve-coverage": "yarn test && browser-sync start --server \"./coverage/lcov-report\" -w",
    "doc": "rimraf ./docs && jsdoc -c \"./jsdoc.config.json\" ",
    "test": "jest --coverage "
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@pixi/jsdoc-template": "^2.4.2",
    "autoprefixer": "^9.4.4",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.0-beta",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.0",
    "docdash": "^1.0.2",
    "eslint": "^5.11.0",
    "eslint-config-google": "^0.11.0",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-loader-jest": "^0.2.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "jest-canvas-mock": "^2.0.0-beta.1",
    "jsdoc": "^3.5.5",
    "nodemon": "^1.18.9",
    "postcss-loader": "^3.0.0",
    "prettier": "1.15.3",
    "rimraf": "^2.6.3",
    "to-string-loader": "^1.1.5",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  },
  "husky": {
    "hooks": {
      "pre-commit": "concurrently \"yarn test\" \"yarn lint --fix\" \"yarn doc\" \"yarn build\" \"git add .\""
    }
  },
  "repository": "https://github.com/softberry/memory-game.git",
  "homepage": "https://softberry.github.io/memory-game/",
  "dependencies": {}
}
